@using AutoRest.CSharp.Model
@using AutoRest.CSharp.Azure.Model
@using AutoRest.Core.Utilities

@inherits AutoRest.Core.Template<CodeModelCs>
@Header("// ")
@EmptyLine
namespace @Settings.Namespace
{
    using System.Linq;
    using Microsoft.Rest;
    using Models;
@EmptyLine
    /// <summary>
    /// Operations.
    /// </summary>
    internal partial class Operations : Microsoft.Rest.IServiceOperations<@Model.Name>, IOperations
    {
        /// <summary>
        /// Initializes a new instance of the Operations class.
        /// </summary>
        /// <param name='client'>
        /// Reference to the service client.
        /// </param>
        /// <exception cref="System.ArgumentNullException">
        /// Thrown when a required parameter is null
        /// </exception>
        internal Operations(@Model.Name client)
        {
            if (client == null) 
            {
                throw new System.ArgumentNullException("client");
            }
            this.Client = client;
        }
        @EmptyLine
        /// <summary>
        /// Gets a reference to the @Model.Name
        /// </summary>
        public @Model.Name Client { get; private set; }
        @EmptyLine
        @foreach (var method in Model.Methods)
        {
        @:@(Include(DependencyInjection.New<AzureMethodTemplate>(), (MethodCs)method))
        @EmptyLine
        }
    }
}